{
  "name": "plataforma-ensino-habilidade",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --config next.config.dev.mjs",
    "build": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=3072' next build",
    "build:dev": "NODE_OPTIONS='--max-old-space-size=1024' next build --config next.config.dev.mjs",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=1024' NEXT_CONFIG_PATH=next.config.dev.mjs next build",
    "build:production": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=2048' next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --dir src --fix",
    "type-check": "tsc --noEmit --incremental",
    "type-check:watch": "tsc --noEmit --watch",
    "check:fast": "next lint --dir src --max-warnings 0",
    "check:all": "npm run check:fast && npm run type-check",
    "check:errors": "next build --dry-run || true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:auth": "node tests/auth-redirect.test.js",
    "analyze": "ANALYZE=true npm run build:production",
    "analyze:bundle": "npx @next/bundle-analyzer",
    "analyze:dev": "ANALYZE=true npm run build:dev",
    "analyze:chunks": "ANALYZE=true BUNDLE_ANALYZE=true npm run build:production",
    "optimize:images": "node scripts/optimize-images.js",
    "clean": "rm -rf .next node_modules/.cache tsconfig.tsbuildinfo",
    "clean:build": "rm -rf .next",
    "clean:all": "rm -rf .next node_modules/.cache tsconfig.tsbuildinfo dist out",
    "build:clean": "npm run clean:build && npm run build",
    "dev:turbo": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "precommit": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@hookform/resolvers": "^3.3.0",
    "@mux/mux-player-react": "^2.6.0",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@sentry/nextjs": "^7.104.0",
    "@supabase/auth-ui-react": "^0.4.6",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@tiptap/extension-underline": "^2.25.0",
    "@tiptap/react": "^2.25.0",
    "@tiptap/starter-kit": "^2.25.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "cmdk": "^1.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^10.16.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "^14.2.30",
    "pdfjs-dist": "^5.3.31",
    "phosphor-react": "^1.4.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.0",
    "react-pdf": "^10.0.1",
    "react-player": "^2.16.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.11.6",
    "@next/bundle-analyzer": "^15.3.5",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "dotenv": "^16.6.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.40.0",
    "postcss": "^8",
    "puppeteer": "^24.12.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.6.0",
    "typescript": "~5.4.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
